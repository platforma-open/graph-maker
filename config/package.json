{
  "name": "block-config",
  "version": "1.0.0",
  "description": "Block config",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup && vite build && block-builder build-configs"
  },
  "dependencies": {
    "@milaboratory/sdk-ui": "^0.12.7"
  },
  "devDependencies": {
    "@milaboratory/block-builder": "^1.2.2",
    "typescript": "^5.5.3",
    "tsup": "8.1.2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
